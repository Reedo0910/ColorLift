{
  "name": "colorlift",
  "productName": "ColorLift",
  "version": "0.2.7",
  "description": "Turning colors into vivid words.",
  "repository": {
    "url": "https://github.com/Reedo0910/ColorLift",
    "type": "git"
  },
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build:win": "electron-forge make --arch=x64 --targets=@electron-forge/maker-squirrel,@electron-forge/maker-zip",
    "build:macos": "electron-forge make --arch=arm64 && electron-forge make --arch=x64",
    "publish": "electron-forge publish",
    "push-with-tags": "git push --follow-tags"
  },
  "type": "module",
  "author": "Zeee",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-dmg": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/publisher-github": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^33.2.1"
  },
  "dependencies": {
    "color-convert": "^2.0.1",
    "color-string": "^1.9.1",
    "dotenv": "^16.4.7",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0",
    "fast-average-color-node": "^3.1.0",
    "i18next": "^24.1.0",
    "screenshot-desktop": "^1.15.0",
    "semver": "^7.6.3",
    "sharp": "^0.33.5"
  },
  "optionalDependencies": {
    "appdmg": "^0.6.6"
  }
}
